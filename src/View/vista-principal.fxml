<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="vistaPrincipal" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.VistaPrincipalController">
   <top>
      <MenuBar fx:id="menuBarPrincipal" visible="false" BorderPane.alignment="CENTER">
        <menus>
          <Menu fx:id="mnuAgregar" mnemonicParsing="false" text="Agregar">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#mnuAgregarTrabajador_action" text="Trabajador" />
                  <MenuItem mnemonicParsing="false" onAction="#mnuAgregarTransaccion_action" text="Transaccion" />
            </items>
          </Menu>
          <Menu fx:id="mnuGenerarReporte" mnemonicParsing="false" text="Generar reporte">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#mnuReporteTrabajadores_action" text="Trabajadores" />
                  <MenuItem mnemonicParsing="false" onAction="#mnuReporteTransaccion_action" text="Transaccion" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
