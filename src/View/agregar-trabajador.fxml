<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AgregarTrabajadorController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
         <children>
            <Label alignment="TOP_LEFT" text="Registrar Trabajador ">
               <padding>
                  <Insets bottom="20.0" right="20.0" top="20.0" />
               </padding>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <GridPane prefWidth="460.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="274.0" minWidth="10.0" prefWidth="251.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="444.0" minWidth="10.0" prefWidth="309.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label prefHeight="17.0" prefWidth="288.0" text="Nombres:" GridPane.rowIndex="1" />
                  <Label prefHeight="17.0" prefWidth="255.0" text="ID:" textAlignment="CENTER" />
                  <Label prefHeight="17.0" prefWidth="254.0" text="Apellidos:" GridPane.rowIndex="2" />
                  <Label prefHeight="17.0" prefWidth="255.0" text="Celular:" GridPane.rowIndex="3" />
                  <Label prefHeight="17.0" prefWidth="252.0" text="Fecha de registro:" textAlignment="CENTER" GridPane.rowIndex="4" />
                  <TextField fx:id="txtIdTrabajador" prefHeight="25.0" prefWidth="312.0" GridPane.columnIndex="1" />
                  <TextField fx:id="txtNombresTrabajador" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="txtApellidosTrabajador" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <TextField fx:id="txtCelularTrabajador" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <DatePicker fx:id="calendarFechaDeRegistro" GridPane.columnIndex="1" GridPane.rowIndex="4" />
               </children>
               <padding>
                  <Insets left="20.0" />
               </padding>
            </GridPane>
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
              <buttons>
                <Button mnemonicParsing="false" onAction="#btnGuardarTrabajador_action" text="Guardar" />
              </buttons>
            </ButtonBar>
         </children></VBox>
   </children>
</AnchorPane>
