                  <JFXTextField fx:id="txtIdentificacion" focusColor="#03fc84" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <JFXTextField fx:id="txtCelular" focusColor="#03fc84" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <JFXPasswordField fx:id="txtContrasena" focusColor="#03fc84" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <JFXPasswordField fx:id="txtConfirmarContrasena" focusColor="#03fc84" GridPane.columnIndex="1" GridPane.rowIndex="5" />
               </children>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </GridPane>
            <Label text="Informacion Finca" textFill="#02b354">
