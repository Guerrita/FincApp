<AnchorPane fx:id="contenedorPrincipal" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.RegistrarAdminFincaController">
   <children>
