<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="vistaPrincipal" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.VistaPrincipalController">
   <top>
      <MenuBar fx:id="menuBarPrincipal" style="-fx-background-color: #02b354; -fx-border-color: black; " visible="false" BorderPane.alignment="CENTER">
        <menus>
          <Menu fx:id="mnuAgregar" mnemonicParsing="false" style="-fx-background-color: #02b354;" text="Agregar">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#mnuAgregarTrabajador_action" text="Trabajador" />
                  <MenuItem mnemonicParsing="false" onAction="#mnuAgregarTransaccion_action" text="Transaccion" />
            </items>
          </Menu>
          <Menu fx:id="mnuGenerarReporte" mnemonicParsing="false" style="-fx-background-color: #02b354;" text="Generar reporte">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#mnuReporteTrabajadores_action" text="Trabajadores" />
                  <MenuItem mnemonicParsing="false" onAction="#mnuReporteTransaccion_action" text="Transaccion" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
